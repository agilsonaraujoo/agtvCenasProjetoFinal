# Chaves de API
REACT_APP_TMDB_API_KEY=SUA_CHAVE_DA_API_TMDB
REACT_APP_GEMINI_API_KEY=SUA_CHAVE_DA_API_GEMINI

# Configurações de API
REACT_APP_API_URL=http://localhost:3000
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_TIMEOUT=5000
REACT_APP_API_RETRY_COUNT=3
REACT_APP_API_RETRY_DELAY=1000

# Configurações de cache
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_TTL=3600000
REACT_APP_CACHE_SIZE=100

# Configurações de segurança
REACT_APP_CORS_ENABLED=true
REACT_APP_CORS_ORIGINS="*"
REACT_APP_CORS_METHODS="GET,HEAD,PUT,PATCH,POST,DELETE"
REACT_APP_CORS_HEADERS="Content-Type,Authorization"
